<?xml version="1.0" ?>
<robot name="testing_rig" xmlns:xacro="http://www.ros.org/wiki/xacro" >

<transmission name="outer_ring_tran">
  <type>transmission_interface/SimpleTransmission</type>
  <joint name="outer_ring">
    <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
  </joint>
  <actuator name="outer_ring_actr">
    <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    <mechanicalReduction>1</mechanicalReduction>
  </actuator>
</transmission>

<transmission name="middle_ring_tran">
  <type>transmission_interface/SimpleTransmission</type>
  <joint name="middle_ring">
    <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
  </joint>
  <actuator name="middle_ring_actr">
    <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    <mechanicalReduction>1</mechanicalReduction>
  </actuator>
</transmission>

<transmission name="inner_ring_tran">
  <type>transmission_interface/SimpleTransmission</type>
  <joint name="inner_ring">
    <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
  </joint>
  <actuator name="inner_ring_actr">
    <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    <mechanicalReduction>1</mechanicalReduction>
  </actuator>
</transmission>

